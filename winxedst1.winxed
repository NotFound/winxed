        int nlems = elements(nskey);
        if (nlems < 1)
            Expected('namespace identifier', start);
        var nssym = owner.findns(toIdentifierList(nskey));

        if (nssym == null)
            SyntaxError('unknow namespace', start);
        owner.usenamespace(nssym);

        ExpectOp(';', tk);
    }
    function optimize()
    {
        return self;
    }
    function emit(e)
    {
    }
}

